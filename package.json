{
  "name": "stock-market-dashboard",
  "version": "1.0.0",
  "description": "To make it easy for you to get started with GitLab, here's a list of recommended next steps.",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --config ./webpack.config.js --mode development",
    "lint": "eslint .",
    "lint:fix": "eslint . --ext .js,.jsx, --fix",
    "format": "prettier --write \"src/**/*.+(js|jsx|json|css|md)\"",
    "build": "webpack --mode production",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run format"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/SashoT/stock-market-dashboard.git"
  },
  "keywords": [],
  "author": "Sasho",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/SashoT/stock-market-dashboard/issues"
  },
  "homepage": "https://gitlab.com/SashoT/stock-market-dashboard#readme",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.7",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-webpack-plugin": "^3.1.1",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "react-hot-loader": "^4.13.0",
    "react-refresh": "^0.11.0",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.7",
    "axios": "^0.24.0",
    "axios-cache-adapter": "^2.7.3",
    "canvasjs-react-charts": "^1.0.5",
    "localforage": "^1.10.0",
    "plotly.js": "^2.8.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-plotly.js": "^2.5.1",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
