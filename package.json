{
  "name": "stock-market-dashboard",
  "version": "1.0.0",
  "description": "To make it easy for you to get started with GitLab, here's a list of recommended next steps.",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --config ./webpack.config.js --mode development",
    "lint": "eslint .",
    "lint:fix": "eslint . --ext .js,.jsx, --fix",
    "format": "prettier --write \"src/**/*.+(js|jsx|json|css|md)\"",
    "build": "webpack --mode production",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/jest.setup.js"
    ],
    "moduleNameMapper": {
      "\\.css$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run format"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/SashoT/stock-market-dashboard.git"
  },
  "keywords": [],
  "author": "Sasho",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/SashoT/stock-market-dashboard/issues"
  },
  "homepage": "https://gitlab.com/SashoT/stock-market-dashboard#readme",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.7",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
    "@testing-library/dom": "^8.11.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-webpack-plugin": "^3.1.1",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "msw": "^0.36.5",
    "node-sass": "^7.0.1",
    "prettier": "^2.5.1",
    "react-hot-loader": "^4.13.0",
    "react-refresh": "^0.11.0",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "whatwg-fetch": "^3.6.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.7",
    "@material-ui/core": "^4.12.3",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "assert": "^2.0.0",
    "axios": "^0.24.0",
    "axios-cache-adapter": "^2.7.3",
    "buffer": "^6.0.3",
    "canvasjs-react-charts": "^1.0.5",
    "chart.js": "^3.7.0",
    "icons": "^1.0.0",
    "localforage": "^1.10.0",
    "path": "^0.12.7",
    "plotly.js": "^2.8.3",
    "react": "^17.0.2",
    "react-alice-carousel": "^2.5.1",
    "react-chartjs-2": "^4.0.0",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.22.5",
    "react-html-parser": "^2.0.2",
    "react-plotly.js": "^2.5.1",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1",
    "stream": "0.0.2",
    "styled-components": "^5.3.3",
    "styled-icons": "^10.45.0",
    "styled-react-modal": "^2.1.0"
  }
}
